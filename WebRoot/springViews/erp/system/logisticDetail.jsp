<script type="text/javascript" src="<%=contextPath %>/springViews/js/json.js"/></script>
<script type="text/javascript">
	function doinit(){
		getLogisticsById();
	}
	
	function getLogisticsById(){
		var url = '<%=contextPath%>/SpringR/system/getLogisticsById?id=<%=id%>';
		jQuery.ajax({
			url:url,
			type:'POST',
			success:function(data){
				var json = eval('('+JSON.stringify(data)+')');
				jQuery('#name').val(json.name);
			}
		});
	}
   
</script>
</head>
<body topmargin="5" onload="doinit()">
<table border="0" width="50%" cellspacing="0" cellpadding="3" class="small">
